  "features": {
    "section_number": {
      "translation": "02",
      "notes": "features::section_number"
    },
    "section_title": {
      "translation": "왜 STATUS NETWORK인가요?",
      "notes": "features::section_title"
    },
    "title": {
      "translation": "커뮤니티에 힘을",
