    "sustainable_funding": {
      "title": {
        "translation": "지속 가능한 공공 자금 조달",
        "notes": "features::sustainable_funding::title"
      },
      "description": {
        "translation": "스테이터스 네트워크는 단기 인센티브를 넘어 근본적인 도약을 이룹니다. 네이티브 수익과 수수료로 운영되는 공공 앱 펀딩 풀은 커뮤니티가 관리하며, 온체인 빌더들을 위한 지속적인 자본 배분을 보장합니다.",
        "notes": "features::sustainable_funding::description"
      },
      "step_1": {
        "translation": "사용자들이 수익 창출 자산을 스테이터스 네트워크에 브릿지함으로써, 펀딩 풀에 할당되는 수익을 증가시킵니다.",
        "notes": "features::sustainable_funding::step_1"
      },
      "step_2": {
        "translation": "앱과 게임 개발자들은 풀에서 자금 배분을 신청합니다. 커뮤니티는 다음과 같은 네트워크 기여를 통해 얻을 수 있는 양도 불가능한 거버넌스 토큰인 카르마(Karma)를 통해 배분 안건에 투표합니다.",
        "notes": "features::sustainable_funding::step_2"
      },
      "step_2_staking": {
        "translation": "SNT 스테이킹",
        "notes": "features::sustainable_funding::step_2_staking"
      },
      "step_2_bridging": {
        "translation": "수익 창출 자산 브릿지",
        "notes": "features::sustainable_funding::step_2_bridging"
      },
      "step_2_using": {
        "translation": "앱 사용",
        "notes": "features::sustainable_funding::step_2_using"
      },
      "step_2_premium": {
        "translation": "프리미엄 가스비 지불",
        "notes": "features::sustainable_funding::step_2_premium"
      },
      "step_2_donations": {
        "translation": "펀딩 풀에 직접 기부",
        "notes": "features::sustainable_funding::step_2_donations"
      },
      "step_3": {
        "translation": "펀딩 지원을 받은 프로젝트들이 신규 유저를 끌어들이면, 더 많은 자산 예치와 토큰 교환이 발생하고, 그 결과 펀딩 풀로 돌아갈 수익과 수수료가 늘어납니다.",
        "notes": "features::sustainable_funding::step_3"
      },
      "button": {
        "translation": "카르믹 토크노믹스 알아보기",
        "notes": "features::sustainable_funding::button"
      }
    },
    "gasless_transactions": {
      "title": {
        "translation": "가스리스 트랜잭션",
        "notes": "features::gasless_transactions::title"
      },
      "description_1": {
        "translation": "스테이터스 네트워크에서는 누구나 무료 트랜잭션을 경험할 수 있습니다! L2 운영 비용은 네이티브 수익과 수수료로 해결하므로, 여러분이 앱을 사용하기 위해 가스비를 브릿지할 필요가 없습니다.",
        "notes": "features::gasless_transactions::description_1"
      },
      "description_2": {
        "translation": "앱과 게임 개발자들은 수많은 사용자에게 매끄러운 온보딩 경험을 제공할 수 있습니다.",
        "notes": "features::gasless_transactions::description_2"
      },
      "button": {
        "translation": "작동 방식 알아보기",
        "notes": "features::gasless_transactions::button"
      }
    },
    "performance": {
      "title": {
        "translation": "성능을 위한 구현, 이더리움 기반 보안",
        "notes": "features::performance::title"
      },
      "description": {
        "translation": "이더리움의 강력한 보안과 Linea의 혁신적인 zkEVM 롤업 기술이 만나 초고속 트랜잭션을 제공합니다. 게임, 소셜 미디어, iGaming, 대용량 애플리케이션을 위한 최고의 선택입니다.",
        "notes": "features::performance::description"
      },
      "button": {
        "translation": "Linea에 대해 더 알아보기",
        "notes": "features::performance::button"
      }
    },
    "decentralization": {
      "title": {
        "translation": "탈중앙화를 위한 설계",
        "notes": "features::decentralization::title"
      },
      "description": {
        "translation": "스테이터스와 IFT 생태계의 핵심 가치 위에 세워진 스테이터스 네트워크는 완전한 탈중앙화 L2를 지향합니다. 최종 목표는 중앙화된 통제 없이 커뮤니티가 자유롭게 성장할 수 있는 검열 저항 환경을 만드는 것입니다.",
        "notes": "features::decentralization::description"
      },
      "button": {
        "translation": "더 알아보기",
        "notes": "features::decentralization::button"
      }
    }
