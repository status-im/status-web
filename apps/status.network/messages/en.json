  "features": {
    "section_number": {
      "translation": "02",
      "notes": "features::section_number"
    },
    "section_title": {
      "translation": "WHY STATUS NETWORK?",
      "notes": "features::section_title"
    },
    "title": {
      "translation": "Power to the community",
      "notes": "features::title"
    },
    "sustainable_funding": {
      "title": {
        "translation": "Sustainable public funding",
        "notes": "features::sustainable_funding::title"
      },
      "description": {
        "translation": "Status Network takes a radical step beyond short-term incentives. Powered by native yields and fees, its public app funding pool is governed by the community, ensuring a continuous allocation of capital for onchain builders.",
        "notes": "features::sustainable_funding::description"
      },
      "step_1": {
        "translation": "Users bridge yield-bearing assets to Status Network and increase the yield redirected to the funding pool.",
        "notes": "features::sustainable_funding::step_1"
      },
      "step_2": {
        "translation": "Apps and games apply for an allocation from the pool. The community votes on allocations using Karma, a non-transferrable governance token earned by contributing positively to the network:",
        "notes": "features::sustainable_funding::step_2"
      },
      "step_2_staking": {
        "translation": "Staking SNT",
        "notes": "features::sustainable_funding::step_2_staking"
      },
      "step_2_bridging": {
        "translation": "Bridging yield-bearing tokens",
        "notes": "features::sustainable_funding::step_2_bridging"
      },
      "step_2_using": {
        "translation": "Using apps",
        "notes": "features::sustainable_funding::step_2_using"
      },
      "step_2_premium": {
        "translation": "Paying premium gas",
        "notes": "features::sustainable_funding::step_2_premium"
      },
      "step_2_donations": {
        "translation": "Making direct donations to the funding pool",
        "notes": "features::sustainable_funding::step_2_donations"
      },
      "step_3": {
        "translation": "Every funded project onboards new users, driving additional asset deposits and token swaps - generating more yield and fees to be allocated in the funding pool.",
        "notes": "features::sustainable_funding::step_3"
      },
      "button": {
        "translation": "Discover Karmic Tokenomics",
        "notes": "features::sustainable_funding::button"
      }
    },
    "gasless_transactions": {
      "title": {
        "translation": "Gasless transactions",
        "notes": "features::gasless_transactions::title"
      },
      "description_1": {
        "translation": "On Status Network, anyone can experience free transactions! With the L2 operating costs covered by native yields and fees, no need to bridge gas to use an app.",
        "notes": "features::gasless_transactions::description_1"
      },
      "description_2": {
        "translation": "Apps and games developers can offer frictionless onboarding for their users at scale.",
        "notes": "features::gasless_transactions::description_2"
      },
      "button": {
        "translation": "Learn how it works",
        "notes": "features::gasless_transactions::button"
      }
    },
    "performance": {
      "title": {
        "translation": "Built for performance, secured by Ethereum",
        "notes": "features::performance::title"
      },
      "description": {
        "translation": "Leverage Ethereum's security with Linea's cutting-edge zkEVM rollup technology for high-speed transactions. Perfect for gaming, social, iGaming, and high-throughput applications.",
        "notes": "features::performance::description"
      },
      "button": {
        "translation": "Learn more about Linea",
        "notes": "features::performance::button"
      }
    },
    "decentralization": {
      "title": {
        "translation": "Geared for decentralization",
        "notes": "features::decentralization::title"
      },
      "description": {
        "translation": "Built on the core values of Status and the IFT ecosystem, Status Network is designed to become a truly decentralised L2. The endgame is to provide a censorship-resistant environment where communities can thrive without central points of control.",
        "notes": "features::decentralization::description"
      },
      "button": {
        "translation": "Learn more",
        "notes": "features::decentralization::button"
      }
    }
  },
