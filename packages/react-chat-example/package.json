    "fix:lint": "eslint './{src,test}/**/*.{ts,tsx}' --fix",
    "test": "run-s 'test:*'",
    "test:lint": "eslint './{src,test}/**/*.{ts,tsx}'",
    "test:prettier": "prettier './{src,test}/**/*.{ts,tsx}' \"./*.json\" --list-different"
